{
    "name": "ivory/google-map-bundle",
    "type": "symfony-bundle",
    "description": "Provides a google map integration for Symfony",
    "keywords": [ "google", "map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/framework-bundle": "^4.0 || ^5.0",
        "ivory/google-map": "dev-feature/v4"
    },
    "require-dev": {
        "ivory/serializer-bundle": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.2",
        "symfony/http-client": "^5.3",
        "phpunit/phpunit": "^8.0",
        "phpunit/phpunit-selenium": "^8.0",
        "symfony/cache": "^4.0 || ^5.0",
        "symfony/form": "^4.0 || ^5.0",
        "symfony/phpunit-bridge": "^4.0 || ^5.0",
        "symfony/templating": "^4.0 || ^5.0",
        "symfony/yaml": "^4.0 || ^5.0",
        "twig/twig": "^3.0",
        "dg/bypass-finals": "^1.3",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMapBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivory\\GoogleMapBundle\\Tests\\": "tests/",
            "Ivory\\Tests\\GoogleMap\\": "vendor/ivory/google-map/tests"
        }
    },
    "replace": {
        "egeloen/google-map-bundle": "^3.0.1"
    }
}
